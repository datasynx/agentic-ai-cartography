{
  "name": "@datasynx/agentic-ai-cartography",
  "version": "0.6.0",
  "description": "AI-powered infrastructure cartography CLI — built on Claude Agent SDK",
  "type": "module",
  "bin": {
    "datasynx-cartography": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "postinstall": "node -e \"try{require('child_process').execSync('claude --version',{stdio:'pipe'})}catch{console.warn('\\n⚠ datasynx-cartography requires Claude CLI: npm i -g @anthropic-ai/claude-code\\n')}\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "infrastructure",
    "cartography",
    "discovery",
    "sop",
    "claude",
    "ai-agent",
    "devops",
    "mermaid",
    "backstage",
    "network-topology",
    "observability"
  ],
  "author": {
    "name": "Datasynx AI",
    "url": "https://github.com/datasynx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datasynx/agentic-ai-cartography.git"
  },
  "homepage": "https://github.com/datasynx/agentic-ai-cartography#readme",
  "bugs": {
    "url": "https://github.com/datasynx/agentic-ai-cartography/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "1.0.128",
    "@anthropic-ai/sdk": "^0.74.0",
    "better-sqlite3": "^12.6.0",
    "commander": "^12.1.0",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  }
}
